<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".MainActivity2End">

    <TextView
            android:id="@+id/EndMoney"
            android:layout_width="203dp"
            android:layout_height="61dp"
            android:text="כסף"
            android:textSize="34sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.806"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.059" />


    <TextView
            android:id="@+id/textView2"
            android:layout_width="196dp"
            android:layout_height="63dp"
            android:text="בחר דירה "
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.558"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.151" />

    <GridLayout
            android:id="@+id/houses"
            android:layout_width="400dp"
            android:layout_height="400dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.576"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.498">

        <ImageView
                android:id="@+id/tel_aviv"
                android:layout_width="220dp"
                android:layout_height="220dp"
                android:layout_row="0"
                android:layout_column="0"
                android:layout_marginRight="20dp"
                app:srcCompat="@drawable/tel_aviv"></ImageView>

        <ImageView
                android:id="@+id/beit_shean"
                android:layout_width="200dp"
                android:layout_height="200dp"
                android:layout_row="0"
                android:layout_column="1"
                app:srcCompat="@drawable/beit_shean"></ImageView>
    </GridLayout>
</androidx.constraintlayout.widget.ConstraintLayout>